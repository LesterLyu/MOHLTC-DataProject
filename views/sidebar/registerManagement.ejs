<%- include('../common/dashboardHeader', {title: 'Register management'}); -%>


<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.13.2/css/bootstrap-select.min.css">

<div class="container-fluid">

    <!-- Breadcrumbs-->
    <ol class="breadcrumb">
        <li class="breadcrumb-item">
            <a href="#">Users</a>
        </li>
        <li class="breadcrumb-item active">Manage Request</li>
    </ol>


    <div class="card">
        <div class="card-body">
            <div class="table-responsive">
                <table id="user-table" class="table  table-hover">
                    <thead>
                    <tr>
                        <th>Username</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Organization</th>
                        <th>Role</th>
                        <th>Group Number</th>
                        <th>Email</th>
                        <th>Phone Number</th>
                        <th>Request</th>
                    </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>

    <div id="alert-error" class="alert alert-danger alert-dismissible hide" role="alert">
        <h5 id="alert-error-text"></h5>
        <button type="button" class="close" aria-label="Close" onclick="hideErrorAlert()">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
    <div id="alert" class="alert alert-success alert-dismissible hide" role="alert">
        <h5 id="alert-text"></h5>
        <button type="button" class="close" aria-label="Close" onclick="hideAlert()">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>

</div>

<%- include('../common/dashboardFooter', {js: [
'https://cdn.datatables.net/1.10.19/js/jquery.dataTables.js',
'https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.13.2/js/bootstrap-select.min.js',
'/moh.js/registerManagement.js',

]}); -%>
