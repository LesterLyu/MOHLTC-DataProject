<%- include('./common/header', {title: 'Log In'}); -%>

<div class="container col-sm-8 col-md-6 col-lg-5 col-xl-4 vertical-gap">
    <div class="loginBox">
        <h1><span class="fa fa-sign-in"></span>Choose your Organization</h1>
    </div>
    <div class="box">

        <br/>
        <!-- LOGIN FORM -->
        <form id="organizationForm">
            <div class="form-group">
                <label class="organizationlabel">Username:</label>
                <input type="text" class="form-control" name="username">
            </div>
            <div class="form-group">
                <label class="loginlabel">Password:</label>
                <input type="password" class="form-control" name="password">
            </div>

            <button type="submit" class="btn btn-primary">Next</button>
        </form>


    </div>
    <!--Message Modal-->
    <%- include('./common/messageModal'); -%>
</div>
<script
        src="https://code.jquery.com/jquery-3.3.1.min.js"
        integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
        crossorigin="anonymous"></script>
<script src="/moh.js/login.js"></script>

<%- include('./common/footer'); -%>
