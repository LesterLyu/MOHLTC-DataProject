<%- include('./common/header', {title: 'Fill Workbook'}); -%>

<div class="container col-sm-10 col-md-10 col-lg-10 col-xl-10 vertical-gap">
    <a href="/profile"><h4><span class="fa fa-reply"></span> Profile</h4></a>
    <input class="hide" id="filled-workbook" value="<%=workbook%>">
    <h2>
        <%=workbook%>
    </h2>
    <nav>
        <div class="nav nav-tabs" id="nav-tab">
        </div>
    </nav>
    <div class="tab-content" id="nav-tabContent">

    </div>
    <br>
    <div class="container row align-items-center" id="save-row">
        <div class="col-auto">
            <button type="button" id="save-workbook-btn" class="btn-save btn btn-primary">Save Workbook</button>
        </div>
        <div class="col-auto">
            <div id="status"> </div>
        </div>
    </div>

    <hr>
    <div class="container d-none d-md-block">
        <h5>Instruction: </h5>
        <p> Right click to view the menu.</p>
    </div>
</div>

