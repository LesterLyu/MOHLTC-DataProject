<%- include('../common/dashboardHeader', {title: 'Create Organization'}); -%>
<div class="container-fluid">
    <!-- Breadcrumbs-->
    <ol class="breadcrumb">
        <li class="breadcrumb-item">
            <a href="#">User Management</a>
        </li>
        <li class="breadcrumb-item active">Create Organization</li>
    </ol>


    <div id="alert-error" class="alert alert-danger alert-dismissible hide" role="alert">
        <h5 id="alert-error-text"></h5>
        <button type="button" class="close" aria-label="Close" onclick="hideErrorAlert()">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
    <div id="alert" class="alert alert-success alert-dismissible hide" role="alert">
        <h5 id="alert-text"></h5>
        <button type="button" class="close" aria-label="Close" onclick="hideAlert()">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>

    <br>

    <form id="organizationForm" method="post">
        <label for="organization"> New organization name: </label>
        <input name="organization" type="text">
        <Button type="submit" class="btn btn-outline-primary"> Submit </Button>
    </form>

</div>
<%- include('../common/dashboardFooter', {js: [
'/moh.js/createOrganization.js',
]}); -%>
